{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "github>berquerant/renovate-config"
  ],
  "ignorePaths": [
    "tests/**"
  ],
  "github-actions": {
    "enabled": false
  },
  "platformAutomerge": false,
  "baseBranches": [
    "main"
  ],
  "packageRules": [
    {
      "addLabels": [
        "docker"
      ],
      "matchDatasources": [
        "docker"
      ]
    }
  ]
}
